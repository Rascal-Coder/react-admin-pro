{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"lint": {
			"dependsOn": ["^lint"]
		},
		"build:rsbuild": {
			"dependsOn": ["^build:rsbuild"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"dev:rsbuild": {
			"cache": false,
			"persistent": true
		},
		"preview:rsbuild": {
			"dependsOn": ["^preview:rsbuild"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"dev:vite": {
			"cache": false,
			"persistent": true
		},
		"build:vite": {
			"dependsOn": ["^build:vite"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"preview:vite": {
			"dependsOn": ["^preview:vite"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		}
	}
}
